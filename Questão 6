#include <iostream>
using namespace std;

int soma(int a, int b) {
    return a + b;
}

int main() {
    int x, y;
    cout << "Digite o primeiro numero inteiro: ";
    cin >> x;
    cout << "Digite o segundo numero inteiro: ";
    cin >> y;

    int resultado = soma(x, y);
    cout << "A soma de " << x << " e " << y << " Ã©: " << resultado << endl;

    return 0;#include <iostream>
using namespace std;
}
